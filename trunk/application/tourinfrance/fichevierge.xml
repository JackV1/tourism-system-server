<?xml version="1.0" encoding="UTF-8"?>
<tif:OI xsi:schemaLocation="http://www.tourinfrance.net/Tourinfrance3/ http://docs.tourism-system.fr/schema/oi.xsd" 
		xmlns:tif="http://www.tourinfrance.net/Tourinfrance3/" 
		xmlns:tif22="http://www.tourinfrance.net/tourinfrance22/" 
		xmlns:dc="http://purl.org/dc/elements/1.1/" 
		xmlns:dcterms="http://purl.org/dc/terms/" 
		xmlns:sy="http://purl.org/rss/1.0/modules/syndication/" 
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<tif:DublinCore>
		<dc:publisher>Tourism System</dc:publisher>
		<dc:identifier>{identifier}</dc:identifier>
		<dcterms:created>{created}</dcterms:created>
		<dcterms:modified>{created}</dcterms:modified>
		<tif:Classification code="{cleClassification}" xml:lang="fr">{libelleClassification}</tif:Classification>
	</tif:DublinCore>
	<tif:Update/>
	<tif:Multimedia/>
	<tif:Contacts>
		<tif:DetailContact type="04.03.13" libelle="Etab/Lieu/Structure">
			<tif:Adresses>
				<tif:DetailAdresse>
					<tif:CodePostal>{code_postal}</tif:CodePostal>
					<tif:Commune code="{code_commune}">{commune}</tif:Commune>
				</tif:DetailAdresse>
			</tif:Adresses>
		</tif:DetailContact>
	</tif:Contacts>
	<tif:InformationsLegales/>
	<tif:Classements/>
	<tif:PrestationsLiees/>
	<tif:Geolocalisations>
		<tif:DetailGeolocalisation type="08.01.02" libelle="Mono-localisé" xml:lang="fr">
			<tif:Zone type="08.02.07.02">
				<tif:Points>
					<tif:DetailPoint type="08.02.05.11" libelle="Principal" xml:lang="fr">
						<tif:Coordonnees>
							<tif:DetailCoordonnees type="08.02.02.03" libelle="Géographiques (UTM compatible avec GPS)" xml:lang="fr">
								<tif:Latitude>{gpsLat}</tif:Latitude>
								<tif:Longitude>{gpsLng}</tif:Longitude>
							</tif:DetailCoordonnees>
						</tif:Coordonnees>
					</tif:DetailPoint>
				</tif:Points>
			</tif:Zone>
		</tif:DetailGeolocalisation>
	</tif:Geolocalisations>
	<tif:Periodes/>
	<tif:Clienteles/>
	<tif:Langues/>
	<tif:ModesReservations/>
	<tif:Tarifs/>
	<tif:Capacites/>
	<tif:OffresPrestations/>
	<tif:DescriptionsComplementaires/>
	<tif:Thesaurus/>
	<tif:Itineraires/>
	<tif:Plannings/>
</tif:OI>